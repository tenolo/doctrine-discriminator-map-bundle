{
    "name": "tenolo/doctrine-discriminator-map-bundle",
    "description": "",
    "keywords": [
        "doctrine",
        "discriminator",
        "map",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/console": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "doctrine/orm": "~2.5",
        "doctrine/common": "~2.6",
        "tenolo/core-bundle": "~2.0@dev"
    },
    "require-dev": {
        "symfony/assetic-bundle": "~2.7",
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://repo.tenolo.com/repo/private/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\DoctrineDiscriminatorMapBundle\\": "src/",
            "Tenolo\\Bundle\\DoctrineDiscriminatorMapBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
